<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <!--
        Added the entity Establishment.
    -->
    <changeSet id="20210927194620-1" author="jhipster">
        <createTable tableName="establishment">
            <column name="id" type="integer">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="liquidation_time" type="${datetimeType}">
                <constraints nullable="true" />
            </column>
            <column name="name" type="varchar(100)">
                <constraints nullable="true" />
            </column>
            <column name="type" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="neighborhood" type="varchar(25)">
                <constraints nullable="true" />
            </column>
            <column name="address" type="varchar(25)">
                <constraints nullable="true" />
            </column>
            <column name="coljuegos_code" type="varchar(100)">
                <constraints nullable="false" />
            </column>
            <column name="start_time" type="${datetimeType}">
                <constraints nullable="false" />
            </column>
            <column name="close_time" type="${datetimeType}">
                <constraints nullable="false" />
            </column>
            <column name="longitude" type="${floatType}">
                <constraints nullable="true" />
            </column>
            <column name="latitude" type="${floatType}">
                <constraints nullable="true" />
            </column>
            <column name="time_zone" type="${text}">
                <constraints nullable="true" />
            </column>
            <column name="mercantile_registration" type="varchar(100)">
                <constraints nullable="true" />
            </column>
            <column name="operator_id" type="smallInt">
                <constraints nullable="false" />
            </column>
            <column name="municipality_id" type="smallInt">
                <constraints nullable="false" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
        <dropDefaultValue tableName="establishment" columnName="liquidation_time" columnDataType="${datetimeType}"/>
        <dropDefaultValue tableName="establishment" columnName="start_time" columnDataType="${datetimeType}"/>
        <dropDefaultValue tableName="establishment" columnName="close_time" columnDataType="${datetimeType}"/>
    </changeSet>

    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here -->

    <!--
